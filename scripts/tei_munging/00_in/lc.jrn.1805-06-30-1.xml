                     </row>
                     <row role="data">
                        <cell role="data" rows="1" cols="1">28th</cell>
                        <cell role="data" rows="1" cols="1">46 a</cell>
                        <cell role="data" rows="1" cols="1">f</cell>
                        <cell role="data" rows="1" cols="1">S. W.</cell>
                        <cell role="data" rows="1" cols="1">75</cell>
                        <cell role="data" rows="1" cols="1">c a f</cell>
                        <cell role="data" rows="1" cols="1">S W</cell>
                        <cell role="data" rows="1" cols="1">r</cell>
                        <cell role="data" rows="1" cols="1"> </cell>
                        <cell role="data" rows="1" cols="1">2</cell>
                     </row>
                     <row role="data">
                        <cell role="data" rows="1" cols="1">29th</cell>
                        <cell role="data" rows="1" cols="1">47 a</cell>
                        <cell role="data" rows="1" cols="1">r. T &amp; L</cell>
                        <cell role="data" rows="1" cols="1">S. W.</cell>
                        <cell role="data" rows="1" cols="1">77</cell>
                        <cell role="data" rows="1" cols="1">f</cell>
