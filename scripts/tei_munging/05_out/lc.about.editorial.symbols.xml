                  </row>
                  <row role="data">
                     <cell role="desc" rows="1" cols="1">Lad., Latd.</cell>
                     <cell role="desc" rows="1" cols="1">latitude</cell>
                  </row>
                  <row role="data">
                     <cell role="desc" rows="1" cols="1">Longtd.</cell>
                     <cell role="desc" rows="1" cols="1">longitude</cell>
                  </row>
                  <row role="data">
                     <cell role="desc" rows="1" cols="1">m., mts.</cell>
