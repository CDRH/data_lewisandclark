            <note xml:id="n04063007" n="7">In the vicinity of the village of 
        <name type="place" key="Wolcott, Kans.">Wolcott</name>, 
        <name type="place" key="Wyandotte County, Kans.">Wyandotte County</name>, 
        <name type="place" key="Kansas">Kansas</name>.</note>
            <note xml:id="n04063008" n="8">Opposite 
        <name type="place" key="Diamond Island">Diamond Island</name>, no longer in existence, in
        southeastern 
        <name type="place" key="Wyandotte County, Kans.">Wyandotte County</name>, 
        <name type="place" key="Kansas">Kansas</name>.</note>
